{
  "manifest_version": 3,
  "name": "Job Tracker",
  "version": "1.1.0",
  "description": "Job Search Storage for Chrome",
  "permissions": [
    "storage",
    "unlimitedStorage",
    "activeTab",
    "scripting",
    "tabs"
  ],
      "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Job Tracker",
    "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
  },
  "options_page": "options.html"
}